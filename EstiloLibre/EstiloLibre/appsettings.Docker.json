{
     "Autenticacion": {
       "TokenJWT_DuracionTokenEnMinutos": 120,
       "EmisorToken": "${BACKEND_URL}",
       "AudienciaToken": "${BACKEND_URL}",
       "TokenJWT_ClaveSecreta": "${JWT_SECRET_KEY}",
       "DuracionTokenEnSegundosParaServicios": 60
     },

     "AplicacionCliente": {
       "Origen1": "${FRONTEND_URL}",
       "Origen2": "${FRONTEND_URL}",
       "MostrarVisorEntorno": true
     },

     "CapaNegocio": {
       "RutaGestorArchivos": "/app/Adjuntos/",
       "CadenaDeConexion": "${DATABASE_URL}",
       "TimeOutComandosConsultasEnSeg": 60
     },

     "Logging": {
       "LogLevel": {
         "Default": "Information",
         "Microsoft.AspNetCore": "Warning"
       }
     },

     "AllowedHosts": "*"
}